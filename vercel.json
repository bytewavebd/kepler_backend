{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
       
        "src": "/(.*)",
  
        "dest": "index.js",
        "methods":["GET", "POST", "OPTIONS"]
      }
    ],
    "functions": {
    "api/test.js": {
      "maxDuration": 50 
    },
    "api/*.js": {
      "maxDuration": 50 
    },
    "src/api/*.js": {
      "maxDuration": 50 
    }
  }
  }